{
  "name": "@eshop/catalog",
  "version": "0.0.1",
  "description": "catalog",
  "main": "index.js",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "make sure to use the extra -- in your command when using flags for example: npm run migrate:create -- --name initial.ts": "",
    "build": "tsc",
    "start:dev": "nodemon",
    "migrate": "ts-node -r tsconfig-paths/register src/infrastructure/database/migrate.ts",
    "migrate:up": "npm run migrate up",
    "migrate:create": "npm run migrate create -- --folder src/infrastructure/database/migrations --name",
    "-------------------- package management -------------------": "",
    "package:check": "ncu -u",
    "-------------------- test -------------------": "",
    "test": "echo 1",
    "-------------------- lint -------------------": "",
    "lint": "eslint --ext .ts src --report-unused-disable-directives",
    "lint:fix": "eslint --ext .ts src --fix --report-unused-disable-directives"
  },
  "author": "Truong Bui",
  "license": "MIT",
  "dependencies": {
    "@eshop/error-handler": "*",
    "@eshop/logger": "*",
    "@eshop/validator": "*",
    "@sinclair/typebox": "0.26.8",
    "cls-hooked": "4.2.2",
    "compression": "1.7.4",
    "convict": "6.2.4",
    "express": "4.18.2",
    "express-openapi-validator": "5.0.3",
    "http-errors": "2.0.0",
    "lodash": "4.17.21",
    "pg": "8.10.0",
    "pino-http": "8.3.3",
    "pm2": "5.3.0",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.30.0",
    "sequelize-typescript": "2.1.5",
    "swagger-ui-express": "4.6.2",
    "ts-results": "3.3.0",
    "umzug": "3.2.1"
  },
  "devDependencies": {
    "@eshop/eslint-config-standard": "*",
    "@eshop/tsconfig": "*",
    "@types/cls-hooked": "4.3.3",
    "@types/convict": "6.1.1",
    "@types/express": "4.17.17",
    "@types/http-errors": "2.0.1",
    "@types/lodash": "4.14.192",
    "@types/node": "18.15.11",
    "@types/swagger-ui-express": "4.1.3",
    "nodemon": "2.0.22",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0"
  }
}
